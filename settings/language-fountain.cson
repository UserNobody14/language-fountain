# https://github.com/atom/language-gfm/blob/master/settings/gfm.cson

'.source.fountain':
  'autocomplete':
    'symbols':
      character:
        selector: '.dialogue.character'
        typePriority: 2
      scene:
        selector: '.scene.heading'
        typePriority: 1
  'editor':
    'commentStart': '/*'
    'commentEnd' : '*/'
    'softWrap': true
