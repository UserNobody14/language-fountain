# If you want some examples of settings, check out:
# https://github.com/atom/language-gfm/blob/master/settings/gfm.cson

'.source.fountain':
  'editor':
    'commentStart': '/*'
    'commentEnd' : '*/'
    'softWrap': true
  autocomplete:
    symbols:
      class:
        selector: '.dialogue.character.fountain'
        typePriority: 2
      function:
        selector: '.fountain.scene.heading'
        typePriority: 1
